{
  "name": "tipsy",
  "version": "1.0.0-beta",
  "scripts": {
    "start": "vite",
    "dev": "vite --host",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "husky:prepare": "cd .. && husky install frontend/.husky",
    "e2e": "npx cypress open",
    "cert": "mkcert localhost"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.2",
    "mobx": "^6.5.0",
    "mobx-persist-store": "^1.0.6",
    "mobx-react": "^7.3.0",
    "open-props": "^1.3.12",
    "react": "^17.0.0",
    "react-a11y-dialog": "^6.1.3",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.5",
    "react-router-dom": "^6.3.0",
    "react-toastify": "^8.2.0",
    "redux-devtools-extension": "^2.13.9",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@vitejs/plugin-react": "^1.0.0",
    "cypress": "^9.5.2",
    "eslint": "^8.12.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-mobx": "^0.0.8",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "postcss-jit-props": "^1.0.5",
    "redux-devtools": "^3.7.0",
    "typescript": "^4.3.2",
    "vite": "^2.6.4"
  },
  "lint-staged": {
    "*.{html,css}": "prettier --write",
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "volta": {
    "prettier": "2.5.1",
    "node": "16.14.0"
  }
}
